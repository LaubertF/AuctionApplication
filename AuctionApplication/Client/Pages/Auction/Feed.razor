@page "/auction/feed"
@using AuctionApplication.Shared

<h3 class="fw-bold mb-4">Explore Items</h3>

<div class="container">
    <div class="pos-f-t">
        <div class="collapse" id="navbarToggleExternalContent">
            <div class="bg-light element-round p-3 mt-4 mb-1">
                <h6 class="card-title fw-bold mb-3">Categories</h6>
                <div class="row row-cols-auto g-1">
                    <div class="col">
                        <button class="btn-sm btn-outline-primary button-round ms-2">Handmade</button>
                    </div>
                    <div class="col">
                        <button class="btn-sm btn-outline-primary button-round ms-2">Paintings</button>
                    </div>
                    <div class="col">
                        <button class="btn-sm btn-outline-primary button-round ms-2">Electronics</button>
                    </div>
                    <div class="col">
                        <button class="btn-sm btn-outline-primary button-round ms-2">Cars</button>
                    </div>
                    <div class="col">
                        <button class="btn-sm btn-outline-primary button-round ms-2">Antiques</button>
                    </div>
                    <div class="col">
                        <button class="btn-sm btn-outline-primary button-round ms-2">Bijouterie</button>
                    </div>
                </div>
            </div>
        </div>
        <nav class="navbar navbar-special navbar-light justify-content-between p-3 m-2 shadow" style="background-color: #e3f2fd">
            <button class="button-outline btn-outline-info navbar-toggler" type="button" data-toggle="collapse" data-target="#navbarToggleExternalContent" aria-controls="navbarToggleExternalContent" aria-expanded="false" aria-label="Toggle navigation">
                <i class="bi bi-funnel"></i>
            </button>
            <div class="row row-cols-auto g-1">
              <div class="col">
                <input class="form-control mr-sm-2" type="search" placeholder="Search" aria-label="Search">
              </div>
              <div class="col">
                <button class="btn btn-outline-info ms-1" type="submit"><i class="bi bi-search"></i></button>
              </div>
            </div>
        </nav>
    </div>

    <div class="row g-5">
